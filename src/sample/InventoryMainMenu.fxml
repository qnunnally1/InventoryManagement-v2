<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="670.0" prefWidth="900.0" style="-fx-background-color: #EEEACD;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.InventoryMainMenuController">
    <children>
        <ImageView id="threadsLogo" fitHeight="112.0" fitWidth="131.0" layoutX="422.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../resources/needle_and_thread-512.png" />
            </image>
        </ImageView>
        <Label id="threadsLabel" layoutX="299.0" layoutY="150.0" text="Threads Clothing Inventory">
            <font>
                <Font size="28.0" />
            </font>
        </Label>
        <Label layoutX="170.0" layoutY="264.0" text="View Inventory">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Label layoutX="421.0" layoutY="264.0" text="Add Product">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Label layoutX="655.0" layoutY="264.0" text="Search Product">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Button id="viewInventoryBtn" layoutX="165.0" layoutY="303.0" mnemonicParsing="false" prefHeight="143.0" prefWidth="132.0" onAction="#viewInventory">
         <graphic>
            <ImageView id="viewInventoryBtn" fitHeight="125.0" fitWidth="102.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../resources/list.png" />
               </image>
            </ImageView>
