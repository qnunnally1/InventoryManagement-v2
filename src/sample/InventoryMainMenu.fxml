            <ImageView id="addProdBtn" fitHeight="135.0" fitWidth="110.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../resources/add-a-button-icon-64392.png" />
               </image>
            </ImageView>
         </graphic>
        </Button>
        <Button id="searchProductBtn" layoutX="653.0" layoutY="303.0" mnemonicParsing="false" prefHeight="143.0" prefWidth="132.0" onAction="#searchProduct">
         <graphic>
            <ImageView fitHeight="131.0" fitWidth="104.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../resources/search.png" />
               </image>
            </ImageView>
         </graphic>
        </Button>
    </children>
</AnchorPane>
